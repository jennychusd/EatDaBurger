<div class="col-xs-6" id="served">
    <h2>Served</h2>
    {{#each served}}
    <form action="/{{this.id}}?_method=PUT" method="POST">
      <label>{{this.burger}}</label><br>
      <button type="submit">Devour</button>
    </form>
    {{/each}}
</div>
<div class="col-xs-6" id="devoured">
    <h2>Devoured</h2>
    {{#each devoured}}
    <div class="devouredBurgers">
      <p>{{this.burger}}</p>
    </div>
    {{/each}}
</div>